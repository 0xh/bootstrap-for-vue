{
  "name": "bootstrap-for-vue",
  "version": "0.1.16",
  "description": "Bootstrap UI for Vue",
  "author": "Rahul Kadyan <hi@znck.me>",
  "main": "dist/bootstrap-for-vue.js",
  "module": "dist/bootstrap-for-vue.esm.js",
  "style": "dist/bootstrap-for-vue.css",
  "scripts": {
    "build": "node build/build.js",
    "build:dll": "webpack --progress --config build/webpack.dll.config.js",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint --fix --ext js,vue src test/**/*.spec.js test/unit/*.js build",
    "lint:css": "stylelint src/**/*.{vue,css}",
    "lint:staged": "lint-staged",
    "test": "yarn run lint && yarn run test:unit",
    "test:unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "dev": "webpack-dev-server --config build/webpack.test.config.js --open",
    "dev:coverage": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "prepublish": "yarn run build"
  },
  "lint-staged": {
    "*.{vue,js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{vue,css}": [
      "stylefmt",
      "stylelint"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "sifter": "^0.5.2",
    "vue-clickaway": "^2.1.0"
  },
  "peerDependencies": {
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^1.0.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "bootstrap": "next",
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.3",
    "clean-css": "^3.4.23",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "file-loader": "^0.10.0",
    "font-awesome": "^4.7.0",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.26.0",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^2.0.1",
    "lint-staged": "^3.2.6",
    "mocha": "^3.2.0",
    "mocha-css": "^1.0.1",
    "node-sass": "^4.4.0",
    "object-assign": "^4.1.0",
    "postcss": "^5.2.9",
    "postcss-cssnext": "^2.9.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.41.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-vue": "beta",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylefmt": "^5.0.3",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-processor-html": "^1.0.0",
    "tether": "^1.4.0",
    "uglify-js": "latest",
    "vue": "^2.1.10",
    "vue-loader": "^10.0.2",
    "vue-router": "^2.1.3",
    "webpack": "beta",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.3.1"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/addStyles.js",
      "style-loader/addStyles.js",
      "css-loader!mocha-css"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znck/bootstrap-for-vue.git"
  },
  "bugs": {
    "url": "https://github.com/znck/bootstrap-for-vue/issues"
  },
  "homepage": "https://github.com/znck/bootstrap-for-vue#readme",
  "license": {
    "type": "MIT",
    "url": "http://www.opensource.org/licenses/mit-license.php"
  }
}
